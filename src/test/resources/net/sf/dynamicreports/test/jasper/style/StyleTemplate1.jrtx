<jasperTemplate>
	<style name="textStyle" forecolor="#000000">
		<box topPadding="2" leftPadding="2" bottomPadding="2" rightPadding="2"/>
	</style>
	<style name="columnStyle" style="textStyle" italic="true"/>
	<style name="titleStyle0" style="textStyle" bold="true"/>
	<style name="titleStyle" style="titleStyle0" bold="true">
		<box>
			<bottomPen lineWidth="2.0"/>
		</box>
	</style>
	<style name="columnTitleStyle3" style="titleStyle">
		<box>
			<topPen lineWidth="2.0"/>
			<leftPen lineWidth="2.0"/>
			<bottomPen lineWidth="2.0"/>
			<rightPen lineWidth="2.0"/>
		</box>
	</style>
	<style name="subtotalStyle" style="textStyle" backcolor="#FFFF00" bold="true">
		<box>
			<topPen lineWidth="1.0"/>
		</box>
	</style>
	<style name="textFieldStyle" style="textStyle" fontSize="15" bold="true"/>
</jasperTemplate>
